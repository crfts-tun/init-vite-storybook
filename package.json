  "scripts": {
    "dev": "vite",
    "optimize": "vite optimize",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc",
    "build:vite": "vite build --minify",
    "build:css": "tailwindcss build -i ./src/style.css -o ./dist/style.css --minify",
    "build": "npm-run-all build:clean build:vite build:tsc build:css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-docs": "build-storybook --docs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vespa-design-system.js",
      "require": "./dist/vespa-design-system.umd.cjs"
    },
    "./custom-theme.cjs": "./dist/custom-theme.cjs",
